programa {
	funcao inicio() {
		
		real nota1, nota2, nota3, nota4, media
		
		escreva("Digite o a nota da 1a prova: ")
		leia(nota1)
		
		escreva("Digite o a nota da 2a prova: ")
		leia(nota2)
		
		escreva("Digite o a nota da 3a prova: ")
		leia(nota3)
		
		escreva("Digite o a nota da 4a prova: ")
		leia(nota4)
		
		media = (nota1+nota2+nota3+nota4)/4
		
		escreva("A Média Arimética das provas é: ", media)
		
	}
}
